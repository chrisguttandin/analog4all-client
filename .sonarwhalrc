{
    "browserslist": [
        "last 2 Chrome versions",
        "last 2 Edge versions",
        "last 2 Firefox versions",
        "last 2 Opera versions",
        "last 2 Safari versions"
    ],
    "connector": {
        "name": "chrome",
        "options": {
            "waitFor": 1000
        }
    },
    "formatters": [
        "stylish"
    ],
    "ignoredUrls": [
        {
            "domain": ".*fonts\\.googleapis\\.com/.*",
            "rules": [ "*" ]
        }, {
            "domain": ".*fonts\\.gstatic\\.com/.*",
            "rules": [ "*" ]
        }, {
            "domain": ".*github\\.io.*.ico$",
            "rules": [ "content-type" ]
        }
    ],
    "rules": {
        "amp-validator": "off",
        "apple-touch-icons": "off",
        "axe": "error",
        "content-type": [ "error", {
            ".*\\.js": "application/javascript; charset=utf-8"
        } ],
        "disown-opener": "error",
        "highest-available-document-mode": "error",
        "html-checker": "off",
        "image-optimization-cloudinary": "off",
        "manifest-app-name": "error",
        "manifest-exists": "off",
        "manifest-file-extension": "error",
        "manifest-is-valid": "error",
        "meta-charset-utf-8": "error",
        "meta-viewport": "error",
        "no-disallowed-headers": "off",
        "no-friendly-error-pages": "error",
        "no-html-only-headers": "error",
        "no-http-redirects": [ "error", {
            "max-target-redirects": 1
        } ],
        "no-protocol-relative-urls": "error",
        "no-vulnerable-javascript-libraries": "error",
        "ssllabs": "error",
        "strict-transport-security": "off",
        "validate-set-cookie-header": "error",
        "x-content-type-options": "off"
    },
    "rulesTimeout": 120000
}
