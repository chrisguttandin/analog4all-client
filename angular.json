{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "defaultProject": "analog4all-client",
    "newProjectRoot": "projects",
    "projects": {
        "analog4all-client": {
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "configurations": {
                        "production": {
                            "budgets": [
                                {
                                    "baseline": "500kb",
                                    "maximumError": "500kb",
                                    "type": "all"
                                },
                                {
                                    "maximumError": "10kb",
                                    "maximumWarning": "6kb",
                                    "type": "anyComponentStyle"
                                }
                            ],
                            "buildOptimizer": true,
                            "extractLicenses": true,
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.prod.ts"
                                }
                            ],
                            "namedChunks": false,
                            "ngswConfigPath": "src/ngsw-config.json",
                            "optimization": true,
                            "outputHashing": "all",
                            "serviceWorker": true,
                            "sourceMap": {
                                "hidden": true,
                                "scripts": true,
                                "styles": false
                            },
                            "vendorChunk": false
                        }
                    },
                    "options": {
                        "aot": true,
                        "assets": ["src/assets"],
                        "index": "src/index.html",
                        "main": "src/main.ts",
                        "outputPath": "build/analog4all-client",
                        "polyfills": "src/polyfills.ts",
                        "scripts": [],
                        "styles": ["src/styles/styles.css"],
                        "tsConfig": "src/tsconfig.json"
                    }
                },
                "e2e": {
                    "builder": "@angular-devkit/build-angular:protractor",
                    "configurations": {
                        "production": {
                            "devServerTarget": "analog4all-client:serve:production"
                        }
                    },
                    "options": {
                        "devServerTarget": "analog4all-client:serve",
                        "protractorConfig": "config/protractor/e2e.js"
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "analog4all-client:build"
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "configurations": {
                        "test": {
                            "tsConfig": ["test/e2e/tsconfig.json", "test/unit/tsconfig.json"],
                            "tslintConfig": "config/tslint/test.json"
                        }
                    },
                    "options": {
                        "exclude": ["**/node_modules/**"],
                        "tsConfig": "src/tsconfig.json",
                        "tslintConfig": "config/tslint/src.json"
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "configurations": {
                        "production": {
                            "browserTarget": "analog4all-client:build:production"
                        }
                    },
                    "options": {
                        "browserTarget": "analog4all-client:build",
                        "port": 6699
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "assets": ["src/assets"],
                        "karmaConfig": "config/karma/config.js",
                        "main": "config/karma/test.ts",
                        "polyfills": "src/polyfills.ts",
                        "scripts": [],
                        "styles": ["src/styles/styles.css"],
                        "tsConfig": "test/unit/tsconfig.json"
                    }
                }
            },
            "prefix": "anc",
            "projectType": "application",
            "root": "",
            "schematics": {},
            "sourceRoot": "src"
        }
    },
    "schematics": {
        "@schematics/angular:class": {
            "skipTests": true
        },
        "@schematics/angular:component": {
            "changeDetection": "OnPush",
            "inlineStyle": false,
            "inlineTemplate": false,
            "prefix": "anc",
            "skipTests": true,
            "style": "css"
        },
        "@schematics/angular:directive": {
            "prefix": "anc",
            "skipTests": true
        },
        "@schematics/angular:interface": {
            "prefix": "I"
        },
        "@schematics/angular:module": {
            "skipTests": true
        },
        "@schematics/angular:pipe": {
            "skipTests": true
        },
        "@schematics/angular:service": {
            "skipTests": true
        }
    },
    "version": 1
}
