{
    "env": {
        "browser": true,
        "node": true
    },
    "parser": "babel-eslint",
    "rules": {
        "block-spacing": [ 2, "always" ],
        "camelcase": 2,
        "curly": 2,
        "eqeqeq": 2,
        "indent": [ 2, 4, { "VariableDeclarator": { "let": 1, "var": 1 } } ],
        "new-cap": 2,
        "no-caller": 2,
        "no-bitwise": 2,
        "no-trailing-spaces": 2,
        "no-undef": 2,
        "no-undefined": 2,
        "no-unused-expressions": 2,
        "no-unused-vars": 2,
        "quotes": [ 2, "single", "avoid-escape" ],
        "space-before-blocks": [ 2, "always" ],
        "strict": [ 2, "global" ],
        "vars-on-top": 2,
        "wrap-iife": 2
    }
}
