{
    "env": {
        "browser": true,
        "node": true
    },
    "parser": "babel-eslint",
    "rules": {
        "camelcase": 2,
        "curly": 2,
        "eqeqeq": 2,
        "indent": 2,
        "new-cap": 2,
        "no-caller": 2,
        "no-bitwise": 2,
        "no-trailing-spaces": 2,
        "no-undef": 2,
        "no-undefined": 2,
        "no-unused-expressions": 2,
        "no-unused-vars": 2,
        "quotes": [ 2, "single", "avoid-escape" ],
        "strict": [ 2, "global" ],
        "wrap-iife": 2
    }
}
